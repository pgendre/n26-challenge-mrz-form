{
  "surname": {
    "regex": "^([a-zA-Z]+[ |'|-|-]?)+$",
    "placeholder": "Surname, ex: Mac O'Neil",
    "error":
      "Required. Max length 30 characters. Letters, spaces or hyphens allowed."
  },
  "givenNames": {
    "regex": "^([a-zA-Z]+[ |'|-|-]?)+$",
    "placeholder": "Given names, ex: John Alexander-Martin",
    "error":
      "Required. Max length 30 characters. Letters, spaces or hyphens allowed."
  },
  "passportNumber": {
    "regex": "^[a-zA-Z0-9]{9}$",
    "placeholder": "Passport Number, ex: AR3598U7V (exactly 9 characters)",
    "error": "Exactly 9 alphanumeric characters."
  },
  "issuingCountry": {
    "placeholder": "Document issuing country",
    "error": "Required."
  },
  "nationality": {
    "placeholder": "User's nationality",
    "error": "Required."
  },
  "dateOfBirth": {
    "error": "Required."
  },
  "dateOfExpiration": {
    "error": "Required. Must occure later than the birth of date."
  }
}
